1.副标题/关键字不能一样

[UIView showInTempWindow:^(UIView *contentView) {
                UIView *coverView = [[UIView alloc] initWithFrame:contentView.bounds];
                coverView.backgroundColor = [UIColor colorWithWhite:0 alpha:0.45];
                coverView.userInteractionEnabled = YES;
                [coverView addGestureRecognizer:[[UITapGestureRecognizer alloc] initWithActionBlock:^(id  _Nonnull sender) {
                    [UIView hiddenTempWindow:nil needDefaultAnimated:YES completedBlock:nil];
                }]];
                [contentView addSubview:coverView];

                BargainSharedView *view = [[BargainSharedView alloc] initWithFrame:CGRectZero];
                [contentView addSubview:view];

                [view mas_makeConstraints:^(MASConstraintMaker *make) {
                    make.top.mas_equalTo(contentView).offset(122);
                    make.centerX.mas_equalTo(contentView);
                    make.size.mas_equalTo(CGSizeMake(ZLLayoutFloat(284), ZLLayoutFloat(344)));
                }];

            } rootViewController:nil windowLevel:0 keyWindow:NO];





            [UIView showInTempWindow:^(UIView *contentView) {
                UIView *coverView = [[UIView alloc] initWithFrame:contentView.bounds];
                coverView.backgroundColor = [UIColor colorWithWhite:0 alpha:0.45];
                coverView.userInteractionEnabled = YES;
                [coverView addGestureRecognizer:[[UITapGestureRecognizer alloc] initWithActionBlock:^(id  _Nonnull sender) {
                    [UIView hiddenTempWindow:nil needDefaultAnimated:YES completedBlock:nil];
                }]];
                [contentView addSubview:coverView];

                BargainSharedSuccessView *view = [[BargainSharedSuccessView alloc] initWithFrame:CGRectZero];
                [contentView addSubview:view];

                [view mas_makeConstraints:^(MASConstraintMaker *make) {
                    make.top.mas_equalTo(contentView).offset(162);
                    make.centerX.mas_equalTo(contentView);
                    make.size.mas_equalTo(CGSizeMake(ZLLayoutFloat(255),ZLLayoutFloat(304)));
                }];

            } rootViewController:nil windowLevel:0 keyWindow:NO];



            [UIView showInTempWindow:^(UIView *contentView) {
                UIView *coverView = [[UIView alloc] initWithFrame:contentView.bounds];
                coverView.backgroundColor = [UIColor colorWithWhite:0 alpha:0.45];
                coverView.userInteractionEnabled = YES;
                [coverView addGestureRecognizer:[[UITapGestureRecognizer alloc] initWithActionBlock:^(id  _Nonnull sender) {
                    [UIView hiddenTempWindow:nil needDefaultAnimated:YES completedBlock:nil];
                }]];
                [contentView addSubview:coverView];

                BargainActivityRuleView *view = [[BargainActivityRuleView alloc] initWithFrame:CGRectZero];
                [contentView addSubview:view];

                [view mas_makeConstraints:^(MASConstraintMaker *make) {
                    make.top.mas_equalTo(contentView).offset(122);
                    make.centerX.mas_equalTo(contentView);
                    make.size.mas_equalTo(CGSizeMake(ZLLayoutFloat(275), ZLLayoutFloat(443)));
                }];

            } rootViewController:nil windowLevel:0 keyWindow:NO];